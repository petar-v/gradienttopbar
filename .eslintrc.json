{
  "env": {
    "browser": true,
    "es6": true,
    "jest": true,
    "webextensions": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "no-undef": "off",
    "no-unused-vars": "off",
    "no-underscore-dangle": "off"
  }
}
